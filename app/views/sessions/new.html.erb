<h1><%= t '.Login Page' %></h1>

<%= form_with( scope: :session, url: sessions_path, local: true ) do |f| %>
  <%= f.label :email, 'email：' %>
  <%= f.email_field :email %>

  <br>

  <%= f.label :password, t('password：') %>
  <%= f.password_field :password %>

  <br>
  <%= f.submit 'login' %>

<% end %>
